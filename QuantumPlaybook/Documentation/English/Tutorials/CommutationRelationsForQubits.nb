Cell[TextData[{
 "Let ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["J",
     FontWeight->"Bold"], ":=", 
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["J", "x"], ",", 
      SuperscriptBox["J", "y"], ",", 
      SuperscriptBox["J", "z"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "359a468a-1feb-41d6-a735-c8aea173ac7e"],
 " be the angular momentum operator. Then, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"U", "=", 
    RowBox[{"exp", "(", 
     RowBox[{
      RowBox[{"-", "\[ImaginaryI]"}], " ", "\[Phi]", " ", 
      RowBox[{
       StyleBox["J",
        FontWeight->"Bold"], "\[CenterDot]", 
       StyleBox["n",
        FontWeight->"Bold"]}]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "02d6773e-fa8b-4e16-a2f2-2dc9eff93443"],
 " is the rotation operator around the axis ",
 StyleBox["n",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 " by angle \[Phi]. This implies that the angular momentum operators satisfy \
the following transformation rules. "
}], "TechNoteText",
 CellID->1317681591,ExpressionUUID->"359ce408-b2fc-4287-8ec3-f4c343f9ef29"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"U", " ", 
     SuperscriptBox["J", "\[Nu]"], 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "\[Mu]"], 
     RowBox[{
      SuperscriptBox["J", "\[Mu]"], 
      RowBox[{
       SubscriptBox["R", "\[Mu]\[Nu]"], "(", 
       RowBox[{
        StyleBox["n",
         FontWeight->"Bold"], ",", "\[Phi]"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"a598ed55-3fc7-41d3-bcf7-384dbaaaf2cc"],
 ","
}], "DisplayMath",
 CellID->1994635563,ExpressionUUID->"3f1f5fbf-3ce5-4104-9fdc-076a20a8e26e"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{"R", "(", 
    RowBox[{
     StyleBox["n",
      FontWeight->"Bold"], ",", "\[Phi]"}], ")"}], TraditionalForm]],
  ExpressionUUID->"a9d4c77b-beda-4869-b2fa-265882371815"],
 " is the three-dimensional rotation matrix around axis ",
 StyleBox["n",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 " by angle \[Phi]."
}], "TechNoteText",
 CellID->1620177671,ExpressionUUID->"a4a4e32c-931f-4de6-997d-fc16cbf9c418"],

