Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"EchoTiming", "[", "\[IndentingNewLine]", 
   RowBox[{"ees", "=", 
    RowBox[{"Table", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"kk", "=", 
        RowBox[{"Range", "[", "k", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Re", "@", 
        RowBox[{"Mean", "@", 
         RowBox[{"WickEntanglementEntropy", "[", 
          RowBox[{"trj", ",", "kk"}], "]"}]}]}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"{", 
