 RowBox[{
  RowBox[{"del", "=", 
   RowBox[{
    RowBox[{"-", "\[CapitalDelta]"}], "*", 
    RowBox[{"One", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"$n", ",", "$n"}], "}"}], ",", "1"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"del", "=", 
   RowBox[{"del", "-", 
    RowBox[{"Transpose", "[", "del", "]"}]}]}], 
