        RowBox[{
         RowBox[{"{", 
          RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"del", "=", 
      RowBox[{"del", "-", 
       RowBox[{"Transpose", "[", "del", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ham", "=", 
