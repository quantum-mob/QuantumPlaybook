 RowBox[{
  RowBox[{"HNKProjective", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"\[Mu]_", ",", "J_", ",", "\[CapitalDelta]_"}], "}"}], ",", 
    "\[Gamma]_", ",", "L_Integer"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"hrm", ",", "del", ",", "ham", ",", "msr"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"hrm", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "\[Mu]"}], "*", 
        RowBox[{"One", "[", "L", "]"}]}], "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"J", "/", "2"}], ")"}], "*", 
        RowBox[{"PlusTopple", "@", 
         RowBox[{"One", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"del", "=", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"(", 
         RowBox[{"\[CapitalDelta]", "/", "2"}], ")"}]}], "*", 
       RowBox[{"One", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"del", "=", 
      RowBox[{"del", "-", 
       RowBox[{"Transpose", "[", "del", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"ham", "=", 
      RowBox[{"NambuHermitian", "[", 
       RowBox[{"{", 
        RowBox[{"hrm", ",", "del"}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"msr", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"One", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], "]"}], ",", 
        RowBox[{"I", "*", 
         RowBox[{"One", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], ",", "1"}], 
          "]"}]}]}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"msr", "=", 
      RowBox[{"ArrayFlatten", "@", 
       RowBox[{"{", 
        RowBox[{"msr", ",", 
         RowBox[{"Reverse", "@", "msr"}]}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"msr", "=", 
      RowBox[{
       RowBox[{"Surd", "[", 
