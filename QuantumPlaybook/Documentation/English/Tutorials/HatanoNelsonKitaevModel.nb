    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"hrm", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "\[Mu]"}], "*", 
        RowBox[{"One", "[", "L", "]"}]}], "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"J", "/", "2"}], ")"}], "*", 
        RowBox[{"PlusTopple", "@", 
