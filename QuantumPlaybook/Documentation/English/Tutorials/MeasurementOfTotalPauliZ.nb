Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "addBitValue", "]"}], ";", 
  RowBox[{
   RowBox[{"addBitValue", "[", "]"}], ":=", 
   RowBox[{"Sequence", "@@", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"addBitValue", "[", "p", "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"p", ",", "$n"}], "}"}]}], 
     "]"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"addBitValue", "[", "p_", "]"}], ":=", 
  RowBox[{"Sequence", "@@", 
   RowBox[{"With", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"logp", "=", 
       RowBox[{"Ceiling", "@", 
        RowBox[{"Log", "[", 
         RowBox[{"2", ",", 
          RowBox[{"p", "+", "1"}]}], "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"CNOT", "[", 
        RowBox[{
         RowBox[{"Prepend", "[", 
          RowBox[{
           RowBox[{"T", "@", 
            RowBox[{"Range", "[", 
             RowBox[{"k", "-", "1"}], "]"}]}], ",", 
           RowBox[{"S", "[", "p", "]"}]}], "]"}], ",", 
         RowBox[{"T", "[", "k", "]"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "logp", ",", "1", ",", 
         RowBox[{"-", "1"}]}], "}"}]}], "]"}]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
 CellLabel->"In[88]:=",
 CellID->590345561,ExpressionUUID->"cf3781bd-b5aa-4fdd-bbfa-98d8624514fe"],

