Cell[BoxData[
 RowBox[{"Needs", "[", "\"\<QuantumMob`Q3`\>\"", "]"}]], "Input",
 CellLabel->"In[1]:=",
 CellID->1242861385,ExpressionUUID->"06263ae8-1b09-4dca-b83f-64e02cbd494a"],

Cell[TextData[{
 "Let us consider a system of ",
 StyleBox["n",
  FontSlant->"Italic"],
 " qubits."
}], "TechNoteText",
 CellID->1846342837,ExpressionUUID->"7eef1a57-3985-4781-ae10-4a96577a97bf"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"$n", "=", "5"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"$l", "=", 
   RowBox[{"Ceiling", "@", 
    RowBox[{"Log", "[", 
     RowBox[{"2", ",", 
      RowBox[{"$n", "+", "1"}]}], "]"}]}]}], ";"}]}], "Input",
 CellLabel->"In[41]:=",
 CellID->1506595512,ExpressionUUID->"41a0ad0f-d9bb-4578-9c8c-5938d70fb0ea"],

Cell[TextData[{
 "We want to measure ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["S", "tot", "z"], ":=", 
    RowBox[{
     SubsuperscriptBox["S", "1", "z"], "+", 
     SubsuperscriptBox["S", "2", "z"], "+", "\[Ellipsis]", "+", 
     SubsuperscriptBox["S", "n", "z"]}]}], TraditionalForm]],ExpressionUUID->
  "fe6c5dd3-cc64-4940-942e-ec7c80f81461"],
 ". The auxiliary register ",
 StyleBox["T",
  FontSlant->"Italic"],
 " is used to store the result."
}], "TechNoteText",
 CellID->1257792721,ExpressionUUID->"16d6c7a8-0057-4b50-81fd-632d04bc3caf"],

Cell[BoxData[{
 RowBox[{"Let", "[", 
  RowBox[{"Qubit", ",", "S", ",", "T"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SS", "=", 
   RowBox[{"S", "[", 
    RowBox[{
     RowBox[{"Range", "@", "$n"}], ",", "None"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"TT", "=", 
   RowBox[{"T", "[", 
    RowBox[{
     RowBox[{"Range", "@", "$l"}], ",", "None"}], "]"}]}], ";"}]}], "Input",
 CellLabel->"In[43]:=",
 CellID->371088407,ExpressionUUID->"eb935d3c-fccb-4e8c-b74f-fb6256bc0813"],

