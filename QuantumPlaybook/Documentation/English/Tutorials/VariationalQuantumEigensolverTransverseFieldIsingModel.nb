Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"rot", ",", "layer"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"rot", "[", "k_", "]"}], "=", 
   RowBox[{"QuantumCircuit", "@", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"EulerRotation", "[", 
       RowBox[{
        RowBox[{"Pi", "*", 
         RowBox[{"a", "[", 
          RowBox[{"k", ",", "q", ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", "2", ",", "3"}], "}"}]}], "]"}]}], ",", 
        RowBox[{"S", "[", 
         RowBox[{"q", ",", "$"}], "]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"q", ",", "$n"}], "}"}]}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"cnt", "=", 
   RowBox[{"QuantumCircuit", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"CNOT", "@@@", 
      RowBox[{"Partition", "[", 
       RowBox[{"SS", ",", "2"}], "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"CNOT", "@@@", 
      RowBox[{"Partition", "[", 
       RowBox[{
        RowBox[{"Rest", "@", "SS"}], ",", "2"}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"layer", "[", "k_Integer", "]"}], "=", 
