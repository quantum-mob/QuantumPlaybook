     "\[IndentingNewLine]", 
     RowBox[{"msr", "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"One", "[", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], "]"}], ",", 
        RowBox[{"I", "*", 
         RowBox[{"One", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"L", "-", "1"}], ",", "L"}], "}"}], ",", "1"}], 
          "]"}]}]}], "}"}]}], ";", "\[IndentingNewLine]", 
