    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"hrm", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "\[Mu]"}], "*", 
        RowBox[{"One", "[", "L", "]"}]}], "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"J", "/", "2"}], ")"}], "*", 
        RowBox[{"PlusTopple", "@", 
         RowBox[{"One", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"del", "=", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"(", 
         RowBox[{"\[CapitalDelta]", "/", "2"}], ")"}]}], "*", 
       RowBox[{"One", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"L", ",", "L"}], "}"}], ",", "1"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"del", "=", 
      RowBox[{"del", "-", 
