Cell["\<\
Here are the quantum jump operators that are linear combinations of the \
fermion creation and annihilation operators.\
\>", "TechNoteMathCaption",
 CellID->1623608584,ExpressionUUID->"60758890-c5ef-480b-9a70-dd0d49a7c162"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"a", "[", "k_", "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"f", "[", "k", "]"}], "+", 
     RowBox[{"I", "*", 
      RowBox[{"Dagger", "[", 
       RowBox[{"f", "[", 
        RowBox[{"k", "+", "1"}], "]"}], "]"}]}]}], ")"}], "/", 
   RowBox[{"Sqrt", "[", "2", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"b", "[", "k_", "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     RowBox[{"f", "[", "k", "]"}], "-", 
     RowBox[{"I", "*", 
      RowBox[{"Dagger", "[", 
       RowBox[{"f", "[", 
        RowBox[{"k", "+", "1"}], "]"}], "]"}]}]}], ")"}], "/", 
   RowBox[{"Sqrt", "[", "2", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ab", "=", 
   RowBox[{"Join", "[", 
    RowBox[{"aa", ",", 
     RowBox[{"Dagger", "@", "bb"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"ab", "//", "TableForm"}]}], "Input",
 CellLabel->"In[11]:=",
 CellID->84230133,ExpressionUUID->"d55ceed4-6636-4e47-b430-dd85c86ee56a"],

