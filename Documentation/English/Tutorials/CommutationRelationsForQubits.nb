    RowBox[{"U", " ", 
     SuperscriptBox["J", "\[Nu]"], 
     SuperscriptBox["U", "\[Dagger]"]}], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "\[Mu]"], 
     RowBox[{
      SuperscriptBox["J", "\[Mu]"], 
      RowBox[{
       SubscriptBox["R", "\[Mu]\[Nu]"], "(", 
       RowBox[{
        StyleBox["n",
         FontWeight->"Bold"], ",", "\[Phi]"}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"07490606-6de2-4b7c-acbe-1ca87b4c19bd"],
 ","
}], "DisplayMath",
 CellID->1994635563,ExpressionUUID->"d245c8e3-0484-484f-bf7f-74a800f411f5"],

Cell[TextData[{
 "where ",
